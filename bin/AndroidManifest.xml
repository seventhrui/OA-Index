    <uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.activity.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.example.activity.ConfigActivity"
            android:label="@string/action_setting"/>
        <activity 
            android:name="com.example.activity.MyCenterActivity" 
            android:label="@string/activity_mycenter"/>
        <activity 
            android:name="com.example.activity.MessageNewActivity"
            android:label="@string/activity_newmessage"/>
        <activity
            android:name="com.example.activity.MessageInboxListAvtivity"
            android:label="@string/activity_inbox" />
        <activity
            android:name="com.example.activity.MessageShowActivity"
            android:label="@string/activity_showmessage" />
