            android:name="com.example.activity.MessageRepliesActivity"
            android:label="@string/action_reply"/>
        <activity 
            android:name="com.example.activity.MessageForwardActivity"
            android:label="@string/action_transpond"/>
        <activity
            android:name="com.example.mytree.ContactsActivity"
            android:label="@string/activity_contacts"/>
        <activity 
            android:name="com.example.activity.StartMenuActivity"
            android:label="@string/action_startmenu"/>
        <activity
            android:name="com.example.activity.RequestLeaveActivity"
            android:label="@string/activity_requestleave"/>
        <activity 
            android:name="com.example.activity.KnowledgeListActivity"
            android:label="@string/knowledge"/>
        <activity 
            android:name="com.example.activity.KnowledgeShowActivity"
            android:label="@string/activity_showknowledge"
